<h1>Welcome to Ascension Test!</h1>
<div class="col-sm-12">
  <%= render 'layouts/messages' %>
</div>
<% if !current_user %>
  <p>
    <%= link_to 'Sign Up', new_user_registration_url %>
    |
    <%= link_to 'Log In', new_user_session_url%>
  </p>
<% else %>
  <% if current_user.admin_area? %>
    <p></p><%= link_to 'Manage Users', users_path %></p>
  <% end %>
  <p>
    <%= link_to 'Sign Out', destroy_user_session_path, method: :delete %>
    |
    <%= link_to 'Edit Account', edit_user_registration_url%>
  </p>
<% end %>
